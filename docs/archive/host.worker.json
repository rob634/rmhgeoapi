{
  "version": "2.0",
  "concurrency": {
    "dynamicConcurrencyEnabled": false,
    "snapshotPersistenceEnabled": false
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Warning",
      "Microsoft.Identity": "Warning",
      "Azure.Identity": "Warning",
      "Azure.Core": "Warning",
      "Azure.Storage": "Warning",
      "MSAL": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:10:00",
  "extensions": {
    "serviceBus": {
      "prefetchCount": 4,
      "messageHandlerOptions": {
        "autoComplete": true,
        "maxConcurrentCalls": 8,
        "maxAutoLockRenewalDuration": "00:10:00"
      }
    }
  }
}
