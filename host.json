{
  "version": "2.0",
  "concurrency": {
    "dynamicConcurrencyEnabled": false,
    "snapshotPersistenceEnabled": false
  },
  "logging": {
    "logLevel": {
      "default": "Debug",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Warning",
      "Microsoft.Identity": "Warning",
      "Azure.Identity": "Warning",
      "Azure.Core": "Warning",
      "Azure.Storage": "Warning",
      "MSAL": "Warning",
      "Function.function_app": "Debug",
      "Function.function_app.User": "Debug"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:30:00",
  "extensions": {
    "serviceBus": {
      "prefetchCount": 8,
      "messageHandlerOptions": {
        "autoComplete": true,
        "maxConcurrentCalls": 8,
        "maxAutoLockRenewalDuration": "00:30:00"
      }
    },
    "queues": {
      "maxPollingInterval": "00:00:02",
      "visibilityTimeout": "00:00:30",
      "batchSize": 2,
      "maxDequeueCount": 1,
      "newBatchThreshold": 1,
      "messageEncoding": "none"
    }
  }
}