{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Warning",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Warning",
      "Microsoft.Identity": "Warning",
      "Azure.Identity": "Warning",
      "Azure.Core": "Warning",
      "Azure.Storage": "Warning",
      "MSAL": "Warning",
      "Function.function_app": "Debug",
      "Function.function_app.User": "Debug"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:05:00",
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:02",
      "visibilityTimeout": "00:00:30",
      "batchSize": 16,
      "maxDequeueCount": 1,  // DEVELOPMENT: Try once, no retries. Production should be 3-5
      "newBatchThreshold": 8,
      "messageEncoding": "none"
    }
  }
}